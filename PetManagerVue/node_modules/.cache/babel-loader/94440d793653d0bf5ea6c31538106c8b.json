DA;AA0DA,IAAA,qBA1DA,iCA0DA,GA1DA,EA0DA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,WAAA,iBAAA,GAAA,GAAA;AACA,KA7DA;AA8DA,IAAA,QA9DA,sBA8DA;AAAA;;AACA,UAAA,CAAA,KAAA,iBAAA,CAAA,MAAA,EAAA;AACA,aAAA,QAAA,CAAA,KAAA,CAAA,YAAA;AACA;AACA;;AACA,UAAA,GAAA,GAAA,KAAA,iBAAA,CAAA,GAAA,CAAA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,EAAA;AAAA,OAAA,CAAA,CALA,CAKA;;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,sBAAA,EAAA,GAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,KAAA,KAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,QAAA;;AACA,UAAA,MAAA,CAAA,IAAA;AACA,SAHA,MAGA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,QAAA;AACA;AACA,OAPA;AAQA,KA5EA;AA6EA,IAAA,KA7EA,mBA6EA;AACA,WAAA,IAAA,GAAA,EAAA;AACA,WAAA,IAAA;AACA,KAhFA;AAiFA,IAAA,gBAjFA,4BAiFA,QAjFA,EAiFA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,WAAA,QAAA,GAAA,QAAA;AACA,WAAA,IAAA;AACA,KArFA;AAsFA,IAAA,mBAtFA,+BAsFA,OAtFA,EAsFA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,OAAA;AACA,WAAA,OAAA,GAAA,OAAA;AACA,WAAA,IAAA;AACA,KA1FA;AA2FA,IAAA,uBA3FA,mCA2FA,GA3FA,EA2FA;AACA,WAAA,IAAA,CAAA,IAAA,GAAA,GAAA;AACA,KA7FA;AA8FA,IAAA,sBA9FA,kCA8FA,GA9FA,EA8FA;AACA,WAAA,IAAA,CAAA,GAAA,GAAA,GAAA;AACA,KAhGA;AAiGA,IAAA,QAjGA,oBAiGA,GAjGA,EAiGA;AACA,MAAA,MAAA,CAAA,IAAA,CAAA,GAAA;AACA,KAnGA;AAoGA,IAAA,GApGA,iBAoGA;AACA,MAAA,MAAA,CAAA,IAAA,CAAA,KAAA,MAAA,CAAA,KAAA,CAAA,OAAA,GAAA,mBAAA;AACA,KAtGA;AAuGA,IAAA,wBAvGA,sCAuGA;AACA,WAAA,QAAA,CAAA,OAAA,CAAA,MAAA;AACA,WAAA,IAAA;AACA;AA1GA;AAtBA,CAAA","sourcesContent":["<template>\r\n  <div style=\"min-height: calc(100vh - 70px)\">\r\n\r\n    <div style=\"margin: 10px 0\">\r\n      <el-button type=\"primary\" @click=\"handleAdd\" > 新增救助信息</el-button>\r\n    </div>\r\n\r\n    <el-table :data=\"tableData\" stripe :show-header=\"false\"  style=\"border-radius: 10px\">\r\n      <el-table-column label=\"图片\" width=\"150\"><template slot-scope=\"scope\"><el-image style=\"width: 100px; height: 100px\"\r\n            :src=\"$store.state.baseApi + scope.row.img\"\r\n            :preview-src-list=\"[$store.state.baseApi + scope.row.img]\"></el-image></template></el-table-column>\r\n      <el-table-column prop=\"information\" label=\"情况描述\"></el-table-column>\r\n      <el-table-column prop=\"address\" label=\"地点\"></el-table-column>\r\n      <el-table-column prop=\"time\" label=\"发现时间\"></el-table-column>\r\n      <el-table-column prop=\"person\" label=\"联系人\"></el-table-column>\r\n      <el-table-column prop=\"phone\" label=\"联系方式\"></el-table-column>\r\n      <el-table-column prop=\"state\" label=\"解决状态\"></el-table-column>\r\n    </el-table>\r\n    <div style=\"margin: 10px 0; padding: 10px; background-color: #fff\">\r\n      <el-pagination  @size-change=\"handleSizeChange\" @current-change=\"handleCurrentChange\"\r\n        :current-page=\"pageNum\" :page-sizes=\"[2, 5, 10, 20]\" :page-size=\"pageSize\" layout=\"total, prev, pager, next\"\r\n        :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n\r\n\r\n    <div style=\"margin: 10px 0\">\r\n      <div class=\"rescueFind\"> 救助站</div>\r\n      <!-- 救助站模块-->\r\n      <rescue></rescue>\r\n    </div>\r\n\r\n    <div style=\"margin: 10px 0\">\r\n      <div class=\"feed\"> 宠物喂养点</div>\r\n      <!-- 救助站模块-->\r\n      <feed></feed>\r\n    </div>\r\n\r\n    <el-dialog title=\"信息\" :visible.sync=\"dialogFormVisible\" width=\"30%\" :close-on-click-modal=\"false\">\r\n      <el-form label-width=\"100px\"  style=\"width: 90%\">\r\n        <el-form-item label=\"情况描述\" >\r\n          <el-input v-model=\"form.information\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"现场照片\">\r\n          <el-upload :action=\"$store.state.baseApi + '/file/upload'\" ref=\"img\" :on-success=\"handleImgUploadSuccess\">\r\n            <el-button  type=\"primary\"> 点击上传</el-button>\r\n          </el-upload>\r\n        </el-form-item>\r\n        <el-form-item label=\"地点\">\r\n          <el-input v-model=\"form.address\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"发现时间\">\r\n          <el-date-picker v-model=\"form.time\" type=\"datetime\" value-format=\"yyyy-MM-dd HH:mm:ss\"\r\n            placeholder=\"选择日期时间\"></el-date-picker>\r\n        </el-form-item>\r\n        <el-form-item label=\"联系人\">\r\n          <el-input v-model=\"form.person\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"联系方式\">\r\n          <el-input v-model=\"form.phone\" autocomplete=\"off\"></el-input>\r\n        </el-form-item>\r\n\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogFormVisible = false\" > 取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"save\" > 确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport rescue from '@/components/front/Rescue.vue'\r\nimport feed from '@/components/front/Feed.vue'\r\nexport default {\r\n  name: \"Salvation\",\r\n  components: {\r\n    rescue,\r\n    feed\r\n  },\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n      total: 0,\r\n      pageNum: 1,\r\n      pageSize: 5,\r\n      name: \"\",\r\n      form: {},\r\n      dialogFormVisible: false,\r\n      multipleSelection: [],\r\n      user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {}\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n  },\r\n  methods: {\r\n    load() {\r\n      this.request.get(\"/salvation/page\", {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n          name: this.name,\r\n        }\r\n      }).then(res => {\r\n        this.tableData = res.data.records\r\n        this.total = res.data.total\r\n      })\r\n    },\r\n    save() {\r\n      this.request.post(\"/salvation\", this.form).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"保存成功\")\r\n          this.dialogFormVisible = false\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"保存失败\")\r\n        }\r\n      })\r\n    },\r\n    handleAdd() {\r\n      this.dialogFormVisible = true\r\n      this.form = {}\r\n      this.$nextTick(() => {\r\n        if (this.$refs.img) {\r\n          this.$refs.img.clearFiles();\r\n        }\r\n        if (this.$refs.file) {\r\n          this.$refs.file.clearFiles();\r\n        }\r\n      })\r\n    },\r\n    handleEdit(row) {\r\n      this.form = JSON.parse(JSON.stringify(row))\r\n      this.dialogFormVisible = true\r\n      this.$nextTick(() => {\r\n        if (this.$refs.img) {\r\n          this.$refs.img.clearFiles();\r\n        }\r\n        if (this.$refs.file) {\r\n          this.$refs.file.clearFiles();\r\n        }\r\n      })\r\n    },\r\n    del(id) {\r\n      this.request.delete(\"/salvation/\" + id).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"删除成功\")\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"删除失败\")\r\n        }\r\n      })\r\n    },\r\n    handleSelectionChange(val) {\r\n      console.log(val)\r\n      this.multipleSelection = val\r\n    },\r\n    delBatch() {\r\n      if (!this.multipleSelection.length) {\r\n        this.$message.error(\"请选择需要删除的数据\")\r\n        return\r\n      }\r\n      let ids = this.multipleSelection.map(v => v.id)  // [{}, {}, {}] => [1,2,3]\r\n      this.request.post(\"/salvation/del/batch\", ids).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"批量删除成功\")\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"批量删除失败\")\r\n        }\r\n      })\r\n    },\r\n    reset() {\r\n      this.name = \"\"\r\n      this.load()\r\n    },\r\n    handleSizeChange(pageSize) {\r\n      console.log(pageSize)\r\n      this.pageSize = pageSize\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      console.log(pageNum)\r\n      this.pageNum = pageNum\r\n      this.load()\r\n    },\r\n    handleFileUploadSuccess(res) {\r\n      this.form.file = res\r\n    },\r\n    handleImgUploadSuccess(res) {\r\n      this.form.img = res\r\n    },\r\n    download(url) {\r\n      window.open(url)\r\n    },\r\n    exp() {\r\n      window.open(this.$store.state.baseApi + \"/salvation/export\")\r\n    },\r\n    handleExcelImportSuccess() {\r\n      this.$message.success(\"导入成功\")\r\n      this.load()\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n\r\n<style>\r\n.headerBg {\r\n  background: #eee !important;\r\n}\r\n\r\n.rescueFind {\r\n  padding: 20px 10px;\r\n  font-size: 28px;\r\n  background-color: #fff;\r\n  border-radius: 10px;\r\n  color: rgb(30, 144, 255);\r\n}\r\n\r\n.feed {\r\n  padding: 20px 10px;\r\n  font-size: 28px;\r\n  background-color: #fff;\r\n  border-radius: 10px;\r\n  color: green;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/front"}]}