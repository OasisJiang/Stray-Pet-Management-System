CAAA,CAAA;AACA,WAAA,iBAAA,GAAA,IAAA;AACA,WAAA,SAAA,CAAA,YAAA;AACA,YAAA,MAAA,CAAA,KAAA,CAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,KAAA,CAAA,GAAA,CAAA,UAAA;AACA;;AACA,YAAA,MAAA,CAAA,KAAA,CAAA,IAAA,EAAA;AACA,UAAA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,UAAA;AACA;AACA,OAPA;AAQA,KAnEA;AAoEA,IAAA,GApEA,eAoEA,EApEA,EAoEA;AAAA;;AACA,WAAA,OAAA,CAAA,MAAA,CAAA,cAAA,EAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,KAAA,KAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,UAAA,MAAA,CAAA,IAAA;AACA,SAHA,MAGA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,MAAA;AACA;AACA,OAPA;AAQA,KA7EA;AA8EA,IAAA,qBA9EA,iCA8EA,GA9EA,EA8EA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,WAAA,iBAAA,GAAA,GAAA;AACA,KAjFA;AAkFA,IAAA,QAlFA,sBAkFA;AAAA;;AACA,UAAA,CAAA,KAAA,iBAAA,CAAA,MAAA,EAAA;AACA,aAAA,QAAA,CAAA,KAAA,CAAA,YAAA;AACA;AACA;;AACA,UAAA,GAAA,GAAA,KAAA,iBAAA,CAAA,GAAA,CAAA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,EAAA;AAAA,OAAA,CAAA,CALA,CAKA;;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,oBAAA,EAAA,GAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,KAAA,KAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,QAAA;;AACA,UAAA,MAAA,CAAA,IAAA;AACA,SAHA,MAGA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,QAAA;AACA;AACA,OAPA;AAQA,KAhGA;AAiGA,IAAA,KAjGA,mBAiGA;AACA,WAAA,IAAA,GAAA,EAAA;AACA,WAAA,IAAA;AACA,KApGA;AAqGA,IAAA,gBArGA,4BAqGA,QArGA,EAqGA;AACA,WAAA,QAAA,GAAA,QAAA;AACA,WAAA,IAAA;AACA,KAxGA;AAyGA,IAAA,mBAzGA,+BAyGA,OAzGA,EAyGA;AACA,WAAA,OAAA,GAAA,OAAA;AACA,WAAA,IAAA;AACA,KA5GA;AA6GA,IAAA,uBA7GA,mCA6GA,GA7GA,EA6GA;AACA,WAAA,IAAA,CAAA,IAAA,GAAA,KAAA,MAAA,CAAA,KAAA,CAAA,OAAA,GAAA,GAAA;AACA,KA/GA;AAgHA,IAAA,sBAhHA,kCAgHA,GAhHA,EAgHA;AACA,WAAA,IAAA,CAAA,GAAA,GAAA,KAAA,MAAA,CAAA,KAAA,CAAA,OAAA,GAAA,GAAA;AACA,KAlHA;AAmHA,IAAA,QAnHA,oBAmHA,GAnHA,EAmHA;AACA,MAAA,MAAA,CAAA,IAAA,CAAA,GAAA;AACA,KArHA;AAsHA,IAAA,GAtHA,iBAsHA;AACA,MAAA,MAAA,CAAA,IAAA,CAAA,KAAA,MAAA,CAAA,KAAA,CAAA,OAAA,GAAA,iBAAA;AACA,KAxHA;AAyHA,IAAA,wBAzHA,sCAyHA;AACA,WAAA,QAAA,CAAA,OAAA,CAAA,MAAA;AACA,WAAA,IAAA;AACA;AA5HA;AApBA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <div style=\"margin: 10px 0\">\r\n      <el-input style=\"width: 200px\" size=\"large\"  placeholder=\"请输入名称\" suffix-icon=\"iconfont icon-r-find\" v-model=\"name\"></el-input>\r\n    </div>\r\n\r\n    <div style=\"margin: 10px 0\">\r\n      <el-button class=\"ml-5\" type=\"primary\" @click=\"load\"> 搜索</el-button>\r\n      <el-button type=\"warning\" @click=\"reset\"> 重置</el-button>\r\n      <el-button type=\"primary\" @click=\"handleAdd\"> 新增 </el-button>\r\n      <el-popconfirm\r\n          class=\"ml-5\"\r\n          confirm-button-text='确定'\r\n          cancel-button-text='我再想想'\r\n          icon=\"el-icon-info\"\r\n          icon-color=\"red\"\r\n          title=\"您确定批量删除这些数据吗？\"\r\n          @confirm=\"delBatch\"\r\n      >\r\n        <el-button type=\"danger\" slot=\"reference\"> 批量删除 </el-button>\r\n      </el-popconfirm>\r\n      <!-- <el-upload :action=\"$store.state.baseApi+'/article/import'\" :show-file-list=\"false\" accept=\"xlsx\" :on-success=\"handleExcelImportSuccess\" style=\"display: inline-block\">\r\n        <el-button type=\"primary\" class=\"ml-5\"> 导入 <i class=\"el-icon-bottom\"></i></el-button>\r\n      </el-upload>\r\n      <el-button type=\"primary\" @click=\"exp\" class=\"ml-5\"> 导出 <i class=\"el-icon-top\"></i></el-button> -->\r\n    </div>\r\n\r\n    <el-table :data=\"tableData\" border stripe :header-cell-class-name=\"'headerBg'\"  @selection-change=\"handleSelectionChange\">\r\n      <el-table-column type=\"selection\" width=\"55\"></el-table-column>\r\n      <el-table-column prop=\"id\" label=\"ID\" width=\"80\" sortable></el-table-column>\r\n      <el-table-column prop=\"name\" label=\"标题\"></el-table-column>\r\n      <el-table-column prop=\"content\" label=\"内容\">\r\n        <template v-slot=\"scope\">\r\n          <el-button  type=\"primary\" @click=\"view(scope.row.content)\"> 查看</el-button>\r\n        </template>\r\n      </el-table-column>\r\n<!--      <el-table-column prop=\"userId\" label=\"\"></el-table-column>-->\r\n      <el-table-column prop=\"time\" label=\"发帖时间\"></el-table-column>\r\n      <el-table-column prop=\"user\" label=\"发帖人\"></el-table-column>\r\n\r\n      <el-table-column label=\"操作\"  fixed=\"right\"   width=\"280\" align=\"center\">\r\n        <template slot-scope=\"scope\">\r\n          <el-button type=\"success\" @click=\"handleEdit(scope.row)\"> 编辑 </el-button>\r\n          <el-popconfirm\r\n              class=\"ml-5\"\r\n              confirm-button-text='确定'\r\n              cancel-button-text='我再想想'\r\n              icon=\"el-icon-info\"\r\n              icon-color=\"red\"\r\n              title=\"您确定删除吗？\"\r\n              @confirm=\"del(scope.row.id)\"\r\n          >\r\n            <el-button type=\"danger\" slot=\"reference\"> 删除 </el-button>\r\n          </el-popconfirm>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <div style=\"padding: 10px 0\">\r\n      <el-pagination\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"pageNum\"\r\n          :page-sizes=\"[2, 5, 10, 20]\"\r\n          :page-size=\"pageSize\"\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n\r\n    <el-dialog title=\"信息\" :visible.sync=\"dialogFormVisible\" width=\"60%\" :close-on-click-modal=\"false\">\r\n      <el-form label-width=\"60px\"  style=\"width: 100%\">\r\n        <el-form-item label=\"标题\">\r\n          <el-input v-model=\"form.name\" autocomplete=\"off\" style=\"width: 500px\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"内容\">\r\n          <mavon-editor ref=\"md\" v-model=\"form.content\" :ishljs=\"true\" @imgAdd=\"imgAdd\"/>\r\n        </el-form-item>\r\n\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogFormVisible = false\">  取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"save\"> 确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n\r\n    <el-dialog title=\"信息\" :visible.sync=\"vis\" width=\"60%\" :close-on-click-modal=\"false\">\r\n      <mavon-editor\r\n          class=\"md\"\r\n          :value=\"content\"\r\n          :subfield=\"false\"\r\n          :defaultOpen=\"'preview'\"\r\n          :toolbarsFlag=\"false\"\r\n          :editable=\"false\"\r\n          :scrollStyle=\"true\"\r\n          :ishljs=\"true\"\r\n      />\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"Article\",\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n      total: 0,\r\n      pageNum: 1,\r\n      pageSize: 10,\r\n      name: \"\",\r\n      form: {},\r\n      dialogFormVisible: false,\r\n      multipleSelection: [],\r\n      user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {},\r\n      vis: false,\r\n      content: ''\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n  },\r\n  methods: {\r\n    view(content) {\r\n      this.content = content\r\n      this.vis = true\r\n    },\r\n    // 绑定@imgAdd event\r\n    imgAdd(pos, $file) {\r\n      let $vm = this.$refs.md\r\n      // 第一步.将图片上传到服务器.\r\n      const formData = new FormData();\r\n      formData.append('file', $file);\r\n      axios({\r\n        url: this.$store.state.baseApi+'/file/upload',\r\n        method: 'post',\r\n        data: formData,\r\n        headers: {'Content-Type': 'multipart/form-data'},\r\n      }).then((res) => {\r\n        // 第二步.将返回的url替换到文本原位置![...](./0) -> ![...](url)\r\n        $vm.$img2Url(pos, this.$store.state.baseApi + res.data);\r\n      })\r\n    },\r\n    load() {\r\n      this.request.get(\"/article/page\", {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n          name: this.name,\r\n        }\r\n      }).then(res => {\r\n        this.tableData = res.data.records\r\n        this.total = res.data.total\r\n      })\r\n    },\r\n    save() {\r\n      this.request.post(\"/article\", this.form).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"保存成功\")\r\n          this.dialogFormVisible = false\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"保存失败\")\r\n        }\r\n      })\r\n    },\r\n    handleAdd() {\r\n      this.dialogFormVisible = true\r\n      this.form = {}\r\n      this.$nextTick(() => {\r\n        if(this.$refs.img) {\r\n           this.$refs.img.clearFiles();\r\n         }\r\n         if(this.$refs.file) {\r\n          this.$refs.file.clearFiles();\r\n         }\r\n      })\r\n    },\r\n    handleEdit(row) {\r\n      this.form = JSON.parse(JSON.stringify(row))\r\n      this.dialogFormVisible = true\r\n       this.$nextTick(() => {\r\n         if(this.$refs.img) {\r\n           this.$refs.img.clearFiles();\r\n         }\r\n         if(this.$refs.file) {\r\n          this.$refs.file.clearFiles();\r\n         }\r\n       })\r\n    },\r\n    del(id) {\r\n      this.request.delete(\"/article/\" + id).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"删除成功\")\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"删除失败\")\r\n        }\r\n      })\r\n    },\r\n    handleSelectionChange(val) {\r\n      console.log(val)\r\n      this.multipleSelection = val\r\n    },\r\n    delBatch() {\r\n      if (!this.multipleSelection.length) {\r\n        this.$message.error(\"请选择需要删除的数据\")\r\n        return\r\n      }\r\n      let ids = this.multipleSelection.map(v => v.id)  // [{}, {}, {}] => [1,2,3]\r\n      this.request.post(\"/article/del/batch\", ids).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"批量删除成功\")\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"批量删除失败\")\r\n        }\r\n      })\r\n    },\r\n    reset() {\r\n      this.name = \"\"\r\n      this.load()\r\n    },\r\n    handleSizeChange(pageSize) {\r\n      this.pageSize = pageSize\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      this.pageNum = pageNum\r\n      this.load()\r\n    },\r\n    handleFileUploadSuccess(res) {\r\n      this.form.file = this.$store.state.baseApi + res\r\n    },\r\n    handleImgUploadSuccess(res) {\r\n      this.form.img = this.$store.state.baseApi + res\r\n    },\r\n    download(url) {\r\n      window.open(url)\r\n    },\r\n    exp() {\r\n      window.open(this.$store.state.baseApi+\"/article/export\")\r\n    },\r\n    handleExcelImportSuccess() {\r\n      this.$message.success(\"导入成功\")\r\n      this.load()\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n\r\n<style>\r\n.headerBg {\r\n  background: #eee!important;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views"}]}