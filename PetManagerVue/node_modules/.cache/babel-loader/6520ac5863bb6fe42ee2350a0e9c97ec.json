A,KAAA,KAAA,CAAA,EAAA,CADA,CAEA;;AACA,UAAA,QAAA,GAAA,IAAA,QAAA,EAAA;AACA,MAAA,QAAA,CAAA,MAAA,CAAA,MAAA,EAAA,KAAA;AACA,MAAA,KAAA,CAAA;AACA,QAAA,GAAA,EAAA,KAAA,MAAA,CAAA,KAAA,CAAA,OAAA,GAAA,cADA;AAEA,QAAA,MAAA,EAAA,MAFA;AAGA,QAAA,IAAA,EAAA,QAHA;AAIA,QAAA,OAAA,EAAA;AAAA,0BAAA;AAAA;AAJA,OAAA,CAAA,CAKA,IALA,CAKA,UAAA,GAAA,EAAA;AACA;AACA,QAAA,GAAA,CAAA,QAAA,CAAA,GAAA,EAAA,MAAA,CAAA,MAAA,CAAA,KAAA,CAAA,OAAA,GAAA,GAAA,CAAA,IAAA;AACA,OARA;AASA,KAhEA;AAiEA,IAAA,IAjEA,kBAiEA;AAAA;;AACA,WAAA,OAAA,CAAA,GAAA,CAAA,eAAA,EAAA;AACA,QAAA,MAAA,EAAA;AACA,UAAA,OAAA,EAAA,KAAA,OADA;AAEA,UAAA,QAAA,EAAA,KAAA,QAFA;AAGA,UAAA,IAAA,EAAA,KAAA;AAHA;AADA,OAAA,EAMA,IANA,CAMA,UAAA,GAAA,EAAA;AACA,QAAA,MAAA,CAAA,SAAA,GAAA,GAAA,CAAA,IAAA,CAAA,OAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,GAAA,CAAA,IAAA,CAAA,KAAA;AACA,OATA;AAUA,KA5EA;AA6EA,IAAA,IA7EA,kBA6EA;AAAA;;AACA,UAAA,CAAA,KAAA,IAAA,CAAA,IAAA,EAAA;AACA,aAAA,QAAA,CAAA,KAAA,CAAA,SAAA;AACA;AACA;;AACA,UAAA,CAAA,KAAA,IAAA,CAAA,OAAA,EAAA;AACA,aAAA,QAAA,CAAA,KAAA,CAAA,SAAA;AACA;AACA;;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,UAAA,EAAA,KAAA,IAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,KAAA,KAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,UAAA,MAAA,CAAA,iBAAA,GAAA,KAAA;;AACA,UAAA,MAAA,CAAA,IAAA;AACA,SAJA,MAIA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,MAAA;AACA;AACA,OARA;AASA,KA/FA;AAgGA,IAAA,SAhGA,uBAgGA;AAAA;;AACA,WAAA,iBAAA,GAAA,IAAA;AACA,WAAA,IAAA,GAAA,EAAA;AACA,WAAA,SAAA,CAAA,YAAA;AACA,YAAA,MAAA,CAAA,KAAA,CAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,KAAA,CAAA,GAAA,CAAA,UAAA;AACA;;AACA,YAAA,MAAA,CAAA,KAAA,CAAA,IAAA,EAAA;AACA,UAAA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,UAAA;AACA;AACA,OAPA;AAQA,KA3GA;AA4GA,IAAA,UA5GA,sBA4GA,GA5GA,EA4GA;AAAA;;AACA,WAAA,IAAA,GAAA,IAAA,CAAA,KAAA,CAAA,IAAA,CAAA,SAAA,CAAA,GAAA,CAAA,CAAA;AACA,WAAA,iBAAA,GAAA,IAAA;AACA,WAAA,SAAA,CAAA,YAAA;AACA,YAAA,MAAA,CAAA,KAAA,CAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,KAAA,CAAA,GAAA,CAAA,UAAA;AACA;;AACA,YAAA,MAAA,CAAA,KAAA,CAAA,IAAA,EAAA;AACA,UAAA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,UAAA;AACA;AACA,OAPA;AAQA,KAvHA;AAwHA,IAAA,GAxHA,eAwHA,EAxHA,EAwHA;AAAA;;AACA,WAAA,OAAA,CAAA,MAAA,CAAA,cAAA,EAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,KAAA,KAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,UAAA,MAAA,CAAA,IAAA;AACA,SAHA,MAGA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,MAAA;AACA;AACA,OAPA;AAQA,KAjIA;AAkIA,IAAA,qBAlIA,iCAkIA,GAlIA,EAkIA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,WAAA,iBAAA,GAAA,GAAA;AACA,KArIA;AAsIA,IAAA,QAtIA,sBAsIA;AAAA;;AACA,UAAA,CAAA,KAAA,iBAAA,CAAA,MAAA,EAAA;AACA,aAAA,QAAA,CAAA,KAAA,CAAA,YAAA;AACA;AACA;;AACA,UAAA,GAAA,GAAA,KAAA,iBAAA,CAAA,GAAA,CAAA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,EAAA;AAAA,OAAA,CAAA,CALA,CAKA;;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,oBAAA,EAAA,GAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,KAAA,KAAA,EAAA;AACA,UAAA,OAAA,CAAA,QAAA,CAAA,OAAA,CAAA,QAAA;;AACA,UAAA,OAAA,CAAA,IAAA;AACA,SAHA,MAGA;AACA,UAAA,OAAA,CAAA,QAAA,CAAA,KAAA,CAAA,QAAA;AACA;AACA,OAPA;AAQA,KApJA;AAqJA,IAAA,KArJA,mBAqJA;AACA,WAAA,IAAA,GAAA,EAAA;AACA,WAAA,IAAA;AACA,KAxJA;AAyJA,IAAA,gBAzJA,4BAyJA,QAzJA,EAyJA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,WAAA,QAAA,GAAA,QAAA;AACA,WAAA,IAAA;AACA,KA7JA;AA8JA,IAAA,mBA9JA,+BA8JA,OA9JA,EA8JA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,OAAA;AACA,WAAA,OAAA,GAAA,OAAA;AACA,WAAA,IAAA;AACA,KAlKA;AAmKA,IAAA,uBAnKA,mCAmKA,GAnKA,EAmKA;AACA,WAAA,IAAA,CAAA,IAAA,GAAA,GAAA;AACA,KArKA;AAsKA,IAAA,sBAtKA,kCAsKA,GAtKA,EAsKA;AACA,WAAA,IAAA,CAAA,GAAA,GAAA,GAAA;AACA,KAxKA;AAyKA,IAAA,QAzKA,oBAyKA,GAzKA,EAyKA;AACA,MAAA,MAAA,CAAA,IAAA,CAAA,GAAA;AACA,KA3KA;AA4KA,IAAA,GA5KA,iBA4KA;AACA,MAAA,MAAA,CAAA,IAAA,CAAA,KAAA,MAAA,CAAA,KAAA,CAAA,OAAA,GAAA,iBAAA;AACA,KA9KA;AA+KA,IAAA,wBA/KA,sCA+KA;AACA,WAAA,QAAA,CAAA,OAAA,CAAA,MAAA;AACA,WAAA,IAAA;AACA;AAlLA;AA5BA,CAAA","sourcesContent":["<template>\r\n  <div style=\"min-height: calc(100vh - 60px); margin: 10px 0\">\r\n    <div style=\"margin: 10px 0\">\r\n      <el-input size=\"large\"  style=\"width: 400px\" placeholder=\"请输入名称\" suffix-icon=\"iconfont icon-r-find\" v-model=\"name\"></el-input>\r\n    </div>\r\n\r\n    <div style=\"margin: 10px 0\">\r\n\r\n      <el-button class=\"ml-5\" type=\"success\" @click=\"load\" > 搜索</el-button>\r\n      <el-button type=\"primary\" class=\"ml-5\"  @click=\"handleAdd\"> 发表帖子</el-button>\r\n    </div>\r\n\r\n    <div style=\"margin: 10px 0;\">\r\n      <div v-for=\"item in tableData\" style=\"padding: 20px; border-radius: 10px; background-color: #fff; margin: 10px 0\" :key=\"item.id\">\r\n        <div style=\"font-size: 20px; font-weight: bold; color: #1E90FF; cursor: pointer\" @click=\"view(item.id, item.content)\">{{ item.name }}</div>\r\n        <div style=\"margin: 15px 0 5px 0; font-size: 14px\">\r\n          <span><i class=\"el-icon-user-solid\" style=\"margin-right: 5px\"></i>{{ item.user }}</span>\r\n          <span><i class=\"el-icon-time\" style=\"margin-right: 5px; margin-left: 20px\"></i>{{ item.time }}</span>\r\n          <span >\r\n            <el-button v-if=\"item.userId === user.id\" type=\"danger\" style=\"float: right; font-size: 16px;  margin-left: 10px\" @click=\"del(item.id)\"> 删除</el-button>\r\n          <el-button v-if=\"item.userId === user.id\" type=\"success\" style=\"float: right; font-size: 16px\" @click=\"handleEdit(item)\"> 编辑</el-button>\r\n          <el-button type=\"primary\" style=\"float: right; font-size: 16px\" @click=\"handleComment(item.id)\"> 评论</el-button>\r\n          </span>\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div  style=\"padding: 10px; margin: 10px 0 30px 0; background-color: #fff; border-radius: 10px\">\r\n      <el-pagination\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"pageNum\"\r\n          :page-sizes=\"[2, 5, 10, 20]\"\r\n          :page-size=\"pageSize\"\r\n          layout=\"total, prev, pager, next\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n    <br>\r\n    <el-dialog title=\"信息\" :visible.sync=\"dialogFormVisible\" width=\"60%\" :close-on-click-modal=\"false\">\r\n      <el-form label-width=\"60px\"  style=\"width: 100%\">\r\n        <el-form-item label=\"标题\">\r\n          <el-input v-model=\"form.name\" autocomplete=\"off\" style=\"width: 500px\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"内容\">\r\n          <mavon-editor ref=\"md\" v-model=\"form.content\" :ishljs=\"true\" @imgAdd=\"imgAdd\"/>\r\n        </el-form-item>\r\n\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogFormVisible = false\">  取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"save\"> 确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n\r\n    <el-dialog title=\"信息\" :visible.sync=\"vis\" width=\"60%\" :close-on-click-modal=\"false\">\r\n      <mavon-editor\r\n          class=\"md\"\r\n          :value=\"content\"\r\n          :subfield=\"false\"\r\n          :defaultOpen=\"'preview'\"\r\n          :toolbarsFlag=\"false\"\r\n          :editable=\"false\"\r\n          :scrollStyle=\"true\"\r\n          :ishljs=\"true\"\r\n      />\r\n\r\n      <div style=\"margin: 30px 0\">\r\n        <div style=\"margin-bottom: 20px; font-size: 20px; border-bottom: 1px solid #ddd; padding-bottom: 10px\">评论列表</div>\r\n        <div  v-for=\"item in comments\" style=\"background-color: #fff; padding: 10px; border-radius: 10px; border-bottom: 1px solid #ddd\" :key=\"item.id\">\r\n          <div style=\"padding: 10px 0; display: flex; color: #666;  font-size: 14px\">\r\n            <div style=\"width: 100px; text-align: center\" >\r\n              <el-image :src=\"$store.state.baseApi+item.avatar\" style=\"width: 50px; height: 50px; border-radius: 50%\"></el-image>\r\n            </div>\r\n            <div style=\"flex: 1; line-height: 25px;\">\r\n              <div>\r\n                <span style=\"color: #3F5EFB\">{{item.user }}：</span>\r\n                <span>{{ item.content }}</span>\r\n              </div>\r\n              <div style=\"font-size: 12px; margin-top: 5px; display: flex\">\r\n                <div style=\"flex: 1\"><i class=\"el-icon-time\"></i> <span>{{ item.time }}</span></div>\r\n                <div style=\"width: 100px; text-align: right\">\r\n                  <el-button type=\"text\" @click=\"delComment(item.id)\" style=\"color: red\" v-if=\"user.nickname === item.user\"> 删除</el-button>\r\n                </div>\r\n              </div>\r\n\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </el-dialog>\r\n\r\n\r\n    <el-dialog title=\"评论\" :visible.sync=\"commentVis\" width=\"50%\" :close-on-click-modal=\"false\">\r\n      <el-form label-width=\"30px\"  style=\"width: 100%\">\r\n        <el-form-item label=\"\">\r\n          <el-input type=\"textarea\" :rows=\"5\" v-model=\"comment.content\" autocomplete=\"off\" style=\"width: 500px\"></el-input>\r\n        </el-form-item>\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"commentVis = false\">  取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"saveComment\"> 确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"Article\",\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n      total: 0,\r\n      pageNum: 1,\r\n      pageSize: 10,\r\n      name: \"\",\r\n      form: {},\r\n      dialogFormVisible: false,\r\n      multipleSelection: [],\r\n      user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {},\r\n      vis: false,\r\n      content: '',\r\n      comment: {},\r\n      commentVis: false,\r\n      comments: []\r\n    }\r\n  },\r\n  created() {\r\n    if (!this.user.id) {\r\n      this.$message.warning(\"请登录\")\r\n      this.$router.push(\"/login\")\r\n      return\r\n    }\r\n    this.load()\r\n  },\r\n  methods: {\r\n    saveComment() {\r\n      if (!this.user.username) {\r\n        this.$message.warning(\"请登录后评论\")\r\n        return\r\n      }\r\n      if (this.comment.contentr) {\r\n        this.comment.content = this.form.contentr\r\n      }\r\n      if (!this.comment.content) {\r\n        this.$message.warning(\"请输入评论\")\r\n        return\r\n      }\r\n      this.comment.type = 2\r\n      this.request.post(\"/comment\", this.comment).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"评论成功\")\r\n          this.commentVis = false\r\n          this.comment = {}  // 清空数据\r\n          this.loadComment()\r\n        } else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    },\r\n    delComment(id) {\r\n      this.request.delete(\"/comment/\" + id).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"删除成功\")\r\n          this.loadComment()\r\n        } else {\r\n          this.$message.error(\"删除失败\")\r\n        }\r\n      })\r\n    },\r\n    loadComment(id) {\r\n      this.request.get(\"/comment/article/2/\" + id).then(res => {\r\n        this.comments = res.data\r\n      })\r\n    },\r\n    handleComment(id) {\r\n      this.comment.articleId = id\r\n      this.commentVis = true\r\n    },\r\n    view(id, content) {\r\n      this.content = content\r\n      this.vis = true\r\n      this.loadComment(id)\r\n    },\r\n    // 绑定@imgAdd event\r\n    imgAdd(pos, $file) {\r\n      let $vm = this.$refs.md\r\n      // 第一步.将图片上传到服务器.\r\n      const formData = new FormData();\r\n      formData.append('file', $file);\r\n      axios({\r\n        url: this.$store.state.baseApi+'/file/upload',\r\n        method: 'post',\r\n        data: formData,\r\n        headers: {'Content-Type': 'multipart/form-data'},\r\n      }).then((res) => {\r\n        // 第二步.将返回的url替换到文本原位置![...](./0) -> ![...](url)\r\n        $vm.$img2Url(pos, this.$store.state.baseApi + res.data);\r\n      })\r\n    },\r\n    load() {\r\n      this.request.get(\"/article/page\", {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n          name: this.name,\r\n        }\r\n      }).then(res => {\r\n        this.tableData = res.data.records\r\n        this.total = res.data.total\r\n      })\r\n    },\r\n    save() {\r\n      if (!this.form.name) {\r\n        this.$message.error(\"标题不能为空！\")\r\n        return\r\n      }\r\n      if (!this.form.content) {\r\n        this.$message.error(\"内容不能为空！\")\r\n        return\r\n      }\r\n      this.request.post(\"/article\", this.form).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"保存成功\")\r\n          this.dialogFormVisible = false\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"保存失败\")\r\n        }\r\n      })\r\n    },\r\n    handleAdd() {\r\n      this.dialogFormVisible = true\r\n      this.form = {}\r\n      this.$nextTick(() => {\r\n        if(this.$refs.img) {\r\n           this.$refs.img.clearFiles();\r\n         }\r\n         if(this.$refs.file) {\r\n          this.$refs.file.clearFiles();\r\n         }\r\n      })\r\n    },\r\n    handleEdit(row) {\r\n      this.form = JSON.parse(JSON.stringify(row))\r\n      this.dialogFormVisible = true\r\n       this.$nextTick(() => {\r\n         if(this.$refs.img) {\r\n           this.$refs.img.clearFiles();\r\n         }\r\n         if(this.$refs.file) {\r\n          this.$refs.file.clearFiles();\r\n         }\r\n       })\r\n    },\r\n    del(id) {\r\n      this.request.delete(\"/article/\" + id).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"删除成功\")\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"删除失败\")\r\n        }\r\n      })\r\n    },\r\n    handleSelectionChange(val) {\r\n      console.log(val)\r\n      this.multipleSelection = val\r\n    },\r\n    delBatch() {\r\n      if (!this.multipleSelection.length) {\r\n        this.$message.error(\"请选择需要删除的数据\")\r\n        return\r\n      }\r\n      let ids = this.multipleSelection.map(v => v.id)  // [{}, {}, {}] => [1,2,3]\r\n      this.request.post(\"/article/del/batch\", ids).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"批量删除成功\")\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"批量删除失败\")\r\n        }\r\n      })\r\n    },\r\n    reset() {\r\n      this.name = \"\"\r\n      this.load()\r\n    },\r\n    handleSizeChange(pageSize) {\r\n      console.log(pageSize)\r\n      this.pageSize = pageSize\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      console.log(pageNum)\r\n      this.pageNum = pageNum\r\n      this.load()\r\n    },\r\n    handleFileUploadSuccess(res) {\r\n      this.form.file = res\r\n    },\r\n    handleImgUploadSuccess(res) {\r\n      this.form.img = res\r\n    },\r\n    download(url) {\r\n      window.open(url)\r\n    },\r\n    exp() {\r\n      window.open(this.$store.state.baseApi+\"/article/export\")\r\n    },\r\n    handleExcelImportSuccess() {\r\n      this.$message.success(\"导入成功\")\r\n      this.load()\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n\r\n<style>\r\n.headerBg {\r\n  background: #eee!important;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views/front"}]}