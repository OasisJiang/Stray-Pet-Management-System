AACA,WAAA,IAAA,GAAA,IAAA,CAAA,KAAA,CAAA,IAAA,CAAA,SAAA,CAAA,GAAA,CAAA,CAAA;AACA,WAAA,iBAAA,GAAA,IAAA;AACA,WAAA,SAAA,CAAA,YAAA;AACA,YAAA,MAAA,CAAA,KAAA,CAAA,GAAA,EAAA;AACA,UAAA,MAAA,CAAA,KAAA,CAAA,GAAA,CAAA,UAAA;AACA;;AACA,YAAA,MAAA,CAAA,KAAA,CAAA,IAAA,EAAA;AACA,UAAA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,UAAA;AACA;AACA,OAPA;AAQA,KApEA;AAqEA,IAAA,GArEA,eAqEA,EArEA,EAqEA;AAAA;;AACA,WAAA,OAAA,CAAA,MAAA,CAAA,gBAAA,EAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,KAAA,KAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,MAAA;;AACA,UAAA,MAAA,CAAA,IAAA;AACA,SAHA,MAGA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,MAAA;AACA;AACA,OAPA;AAQA,KA9EA;AA+EA,IAAA,qBA/EA,iCA+EA,GA/EA,EA+EA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,WAAA,iBAAA,GAAA,GAAA;AACA,KAlFA;AAmFA,IAAA,QAnFA,sBAmFA;AAAA;;AACA,UAAA,CAAA,KAAA,iBAAA,CAAA,MAAA,EAAA;AACA,aAAA,QAAA,CAAA,KAAA,CAAA,YAAA;AACA;AACA;;AACA,UAAA,GAAA,GAAA,KAAA,iBAAA,CAAA,GAAA,CAAA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,EAAA;AAAA,OAAA,CAAA,CALA,CAKA;;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,sBAAA,EAAA,GAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,IAAA,KAAA,KAAA,EAAA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,QAAA;;AACA,UAAA,MAAA,CAAA,IAAA;AACA,SAHA,MAGA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,QAAA;AACA;AACA,OAPA;AAQA,KAjGA;AAkGA,IAAA,KAlGA,mBAkGA;AACA,WAAA,IAAA,GAAA,EAAA;AACA,WAAA,IAAA;AACA,KArGA;AAsGA,IAAA,gBAtGA,4BAsGA,QAtGA,EAsGA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,WAAA,QAAA,GAAA,QAAA;AACA,WAAA,IAAA;AACA,KA1GA;AA2GA,IAAA,mBA3GA,+BA2GA,OA3GA,EA2GA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,OAAA;AACA,WAAA,OAAA,GAAA,OAAA;AACA,WAAA,IAAA;AACA,KA/GA;AAgHA,IAAA,uBAhHA,mCAgHA,GAhHA,EAgHA;AACA,WAAA,IAAA,CAAA,IAAA,GAAA,GAAA;AACA,KAlHA;AAmHA,IAAA,sBAnHA,kCAmHA,GAnHA,EAmHA;AACA,WAAA,IAAA,CAAA,GAAA,GAAA,GAAA;AACA,KArHA;AAsHA,IAAA,QAtHA,oBAsHA,GAtHA,EAsHA;AACA,MAAA,MAAA,CAAA,IAAA,CAAA,GAAA;AACA,KAxHA;AAyHA,IAAA,GAzHA,iBAyHA;AACA,MAAA,MAAA,CAAA,IAAA,CAAA,KAAA,MAAA,CAAA,KAAA,CAAA,OAAA,GAAA,mBAAA;AACA,KA3HA;AA4HA,IAAA,wBA5HA,sCA4HA;AACA,WAAA,QAAA,CAAA,OAAA,CAAA,MAAA;AACA,WAAA,IAAA;AACA;AA/HA;AApBA,CAAA","sourcesContent":["<template>\r\n  <div  style=\"margin: 10px 0\">\r\n\r\n    <div class=\"articleKp\"> 宠物科普</div>\r\n\r\n    <div style=\"margin: 10px 0;\">\r\n      <div v-for=\"item in tableData\" style=\"padding: 20px; border-radius: 10px; background-color: #fff; margin: 10px 0\" :key=\"item.id\">\r\n        <div style=\"display: flex\">\r\n\r\n          <div style=\"flex: 1\">\r\n            <div style=\"font-size: 20px; font-weight: bold; color: #1E90FF; cursor: pointer\" @click=\"view(item.id, item.content)\">{{ item.name }}</div>\r\n\r\n            <div style=\"margin-top: 60px; font-size: 14px\">\r\n              <span><i class=\"el-icon-reading\" style=\"margin-right: 5px\"></i>{{ item.read1 }}</span>\r\n              <span><i class=\"el-icon-time\" style=\"margin-right: 5px; margin-left: 20px\"></i>{{ item.time }}</span>\r\n              <span v-if=\"item.userId === user.id\">\r\n            <el-button type=\"text\" style=\"float: right; font-size: 16px; color: red; margin-left: 10px\" @click=\"del(item.id)\"> 删除</el-button>\r\n            <el-button type=\"text\" style=\"float: right; font-size: 16px\" @click=\"handleEdit(item)\"> 编辑</el-button>\r\n             </span>\r\n            </div>\r\n          </div>\r\n\r\n          <div style=\"width: 200px; text-align: center\">\r\n            <el-image :src=\"$store.state.baseApi+item.img\" style=\"width: 100px; height: 100px\" :preview-src-list=\"[$store.state.baseApi + item.img]\"></el-image>\r\n          </div>\r\n\r\n        </div>\r\n      </div>\r\n    </div>\r\n    <div  style=\"padding: 10px; margin: 10px 0; background-color: #fff; border-radius: 10px\">\r\n      <el-pagination\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :current-page=\"pageNum\"\r\n          :page-sizes=\"[2, 5, 10, 20]\"\r\n          :page-size=\"pageSize\"\r\n          layout=\"total, prev, pager, next\"\r\n          :total=\"total\">\r\n      </el-pagination>\r\n    </div>\r\n\r\n    <el-dialog title=\"信息\" :visible.sync=\"dialogFormVisible\" width=\"60%\" :close-on-click-modal=\"false\">\r\n      <el-form label-width=\"60px\"  style=\"width: 100%\">\r\n        <el-form-item label=\"标题\">\r\n          <el-input v-model=\"form.name\" autocomplete=\"off\" style=\"width: 500px\"></el-input>\r\n        </el-form-item>\r\n        <el-form-item label=\"封面\">\r\n          <el-upload\r\n              class=\"avatar-uploader\"\r\n              :action=\"$store.state.baseApi+'/file/upload'\"\r\n              :show-file-list=\"false\"\r\n              :on-success=\"handleImgUploadSuccess\"\r\n          >\r\n            <img v-if=\"form.img\" :src=\"$store.state.baseApi+form.img\" class=\"avatar\">\r\n            <i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\r\n          </el-upload>\r\n        </el-form-item>\r\n        <el-form-item label=\"内容\">\r\n          <mavon-editor ref=\"md\" v-model=\"form.content\" :ishljs=\"true\" @imgAdd=\"imgAdd\"/>\r\n        </el-form-item>\r\n\r\n      </el-form>\r\n      <div slot=\"footer\" class=\"dialog-footer\">\r\n        <el-button @click=\"dialogFormVisible = false\">  取 消</el-button>\r\n        <el-button type=\"primary\" @click=\"save\"> 确 定</el-button>\r\n      </div>\r\n    </el-dialog>\r\n\r\n    <el-dialog title=\"信息\" :visible.sync=\"vis\" width=\"60%\" :close-on-click-modal=\"false\">\r\n      <mavon-editor\r\n          class=\"md\"\r\n          :value=\"content\"\r\n          :subfield=\"false\"\r\n          :defaultOpen=\"'preview'\"\r\n          :toolbarsFlag=\"false\"\r\n          :editable=\"false\"\r\n          :scrollStyle=\"true\"\r\n          :ishljs=\"true\"\r\n      />\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios.js\";\r\n\r\nexport default {\r\n  name: \"ArticleKp\",\r\n  data() {\r\n    return {\r\n      tableData: [],\r\n      total: 0,\r\n      pageNum: 1,\r\n      pageSize: 5,\r\n      name: \"\",\r\n      form: {},\r\n      dialogFormVisible: false,\r\n      multipleSelection: [],\r\n      user: localStorage.getItem(\"user\") ? JSON.parse(localStorage.getItem(\"user\")) : {},\r\n      vis: false,\r\n      content: ''\r\n    }\r\n  },\r\n  created() {\r\n    this.load()\r\n  },\r\n  methods: {\r\n    view(id, content) {\r\n      this.request.get(\"/articleKp/\" + id)\r\n      this.content = content\r\n      this.vis = true\r\n    },\r\n    // 绑定@imgAdd event\r\n    imgAdd(pos, $file) {\r\n      let $vm = this.$refs.md\r\n      // 第一步.将图片上传到服务器.\r\n      const formData = new FormData();\r\n      formData.append('file', $file);\r\n      axios({\r\n        url: this.$store.state.baseApi+'/file/upload',\r\n        method: 'post',\r\n        data: formData,\r\n        headers: {'Content-Type': 'multipart/form-data'},\r\n      }).then((res) => {\r\n        // 第二步.将返回的url替换到文本原位置![...](./0) -> ![...](url)\r\n        $vm.$img2Url(pos, this.$store.state.baseApi + res.data);\r\n      })\r\n    },\r\n    load() {\r\n      this.request.get(\"/articleKp/page\", {\r\n        params: {\r\n          pageNum: this.pageNum,\r\n          pageSize: this.pageSize,\r\n          name: this.name,\r\n        }\r\n      }).then(res => {\r\n        this.tableData = res.data.records\r\n        this.total = res.data.total\r\n      })\r\n    },\r\n    save() {\r\n      this.request.post(\"/articleKp\", this.form).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"保存成功\")\r\n          this.dialogFormVisible = false\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"保存失败\")\r\n        }\r\n      })\r\n    },\r\n    handleAdd() {\r\n      this.dialogFormVisible = true\r\n      this.form = {}\r\n      this.$nextTick(() => {\r\n        if(this.$refs.img) {\r\n           this.$refs.img.clearFiles();\r\n         }\r\n         if(this.$refs.file) {\r\n          this.$refs.file.clearFiles();\r\n         }\r\n      })\r\n    },\r\n    handleEdit(row) {\r\n      this.form = JSON.parse(JSON.stringify(row))\r\n      this.dialogFormVisible = true\r\n       this.$nextTick(() => {\r\n         if(this.$refs.img) {\r\n           this.$refs.img.clearFiles();\r\n         }\r\n         if(this.$refs.file) {\r\n          this.$refs.file.clearFiles();\r\n         }\r\n       })\r\n    },\r\n    del(id) {\r\n      this.request.delete(\"/articleKp/\" + id).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"删除成功\")\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"删除失败\")\r\n        }\r\n      })\r\n    },\r\n    handleSelectionChange(val) {\r\n      console.log(val)\r\n      this.multipleSelection = val\r\n    },\r\n    delBatch() {\r\n      if (!this.multipleSelection.length) {\r\n        this.$message.error(\"请选择需要删除的数据\")\r\n        return\r\n      }\r\n      let ids = this.multipleSelection.map(v => v.id)  // [{}, {}, {}] => [1,2,3]\r\n      this.request.post(\"/articleKp/del/batch\", ids).then(res => {\r\n        if (res.code === '200') {\r\n          this.$message.success(\"批量删除成功\")\r\n          this.load()\r\n        } else {\r\n          this.$message.error(\"批量删除失败\")\r\n        }\r\n      })\r\n    },\r\n    reset() {\r\n      this.name = \"\"\r\n      this.load()\r\n    },\r\n    handleSizeChange(pageSize) {\r\n      console.log(pageSize)\r\n      this.pageSize = pageSize\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      console.log(pageNum)\r\n      this.pageNum = pageNum\r\n      this.load()\r\n    },\r\n    handleFileUploadSuccess(res) {\r\n      this.form.file = res\r\n    },\r\n    handleImgUploadSuccess(res) {\r\n      this.form.img = res\r\n    },\r\n    download(url) {\r\n      window.open(url)\r\n    },\r\n    exp() {\r\n      window.open(this.$store.state.baseApi+\"/articleKp/export\")\r\n    },\r\n    handleExcelImportSuccess() {\r\n      this.$message.success(\"导入成功\")\r\n      this.load()\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n\r\n<style>\r\n.articleKp {\r\n  padding: 20px 10px;\r\n  font-size: 28px;\r\n  background-color: #fff;\r\n  border-radius: 10px;\r\n  color: rgb(30, 144, 255);\r\n}\r\n.headerBg {\r\n  background: #eee!important;\r\n}\r\n.avatar-uploader {\r\n  padding-left: 20px;\r\n  padding-bottom: 10px;\r\n}\r\n.avatar-uploader .el-upload {\r\n  border: 1px dashed #d9d9d9;\r\n  border-radius: 6px;\r\n  cursor: pointer;\r\n  position: relative;\r\n  overflow: hidden;\r\n}\r\n.avatar-uploader .el-upload:hover {\r\n  border-color: #409EFF;\r\n}\r\n.avatar-uploader-icon {\r\n  font-size: 28px;\r\n  color: #8c939d;\r\n  width: 138px;\r\n  height: 138px;\r\n  line-height: 138px;\r\n  text-align: center;\r\n}\r\n.avatar {\r\n  width: 138px;\r\n  height: 138px;\r\n  display: block;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components/front"}]}